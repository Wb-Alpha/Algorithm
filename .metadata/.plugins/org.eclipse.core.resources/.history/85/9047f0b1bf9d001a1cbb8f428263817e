package Training;

import java.util.*;

public class Interview {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int m = in.nextInt();
		m = (int) (m*1.5);
		int[][] info = new int[n][2];
		
		for (int i=0; i<n; i++) {
			info[i][0] = in.nextInt();
			info[i][1] = in.nextInt();
		}
		
		
		in.close();
	}
}
